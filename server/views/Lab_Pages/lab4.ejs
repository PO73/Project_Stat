<!DOCTYPE html>
<html>
    <head>
        <%- include('lab_head') %>
    </head>
    <body>
        <header>
            <%- include('../global_header') %>
        </header>
        <div class="containerLab">
            <div class="containercontainer">
            <h1><%= typeof Title != 'undefined' ? Title : 'Lab#' %></h1>
            <button type="print" onclick="printPage()" value="Print" class="labButtons">Print</button>
        </div>
        <div id="questionSet">
            <%- include('../partials/displayRewards') %>
            <p><b>Research Scenario: </b> <%= typeof researchScenario != 'undefined' ? researchScenario : 'This is where the research scenario should be...' %></p>
            <p><b>Directions: </b> <%= typeof Direction != 'undefined' ? Direction : 'This is where the directions should be...' %></p>
            <img src=<%= typeof Image != 'undefined' ? Image : '' %> alt="Italian Trulli">
            
            <% if(typeof questionFeedback == 'undefined') { %>
            <form action="/user/lab/gradelab4" method="POST">
                <%- include('../partials/labDisplayQuestion') %>
                <input type="submit" class="labButtons">
            </form>
            <% } else { %>
                <%- include('../partials/labDisplayQuestion') %>
            <% } %>

            <button type="back" onclick="location.href='/user/studentdashboard';" value="Back" class="labButtons">Back</button>
        </div>
        <script type="text/javascript" src="/js/printPage.js"></script>
        <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
        <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
    </body>
</html>