<% if(typeof accountSuccess != 'undefined') { %>
    <div class="alert alert-success" role="alert">
        <p>Update successful!</p>
    </div>
<% }  %>
<% if(typeof accountFail != 'undefined') { %>
    <% for (var i = 0; i < accountFail.length; i++) { %>
        <div class="alert alert-danger" role="alert">
            <%= accountFail[i] %>
        </div>
    <% } %>
<% } %>